{
  "name": "@gctools-components/phrase-cloud",
  "version": "1.1.0",
  "description": "Phrase cloud SVG based component",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gctools-outilsgc/gctools-components.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist --copy-files",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint .",
    "test": "jest --rootDir . --config ./node_modules/@gctools-components/jest-config/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "sso",
    "single sign on",
    "identity server",
    "login",
    "gc",
    "canada",
    "oidc",
    "authentication",
    "nrc"
  ],
  "author": {
    "name": "Luc Belliveau",
    "email": "luc.belliveau@nrc-cnrc.gc.ca"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@gctools-components/eslint-config": "^1.1.0",
    "@gctools-components/jest-config": "^1.1.0",
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-info": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/addon-storyshots": "^3.2.13",
    "@storybook/react": "^3.3.0-alpha.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-react-docgen": "~1.8.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "css-loader": "^0.28.7",
    "d3": "^4.10.0",
    "deep-equal": "^1.0.1",
    "fontfaceobserver": "^2.0.13",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "svg-path-properties": "^0.4.1",
    "uuid": "^3.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie 11"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "react-docgen",
        {
          "includeMethods": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@gctools-components",
    "root": true
  }
}
