{
  "name": "@gctools-components/language-selector",
  "version": "1.1.0",
  "description": "React component designed to work with @gctools-components/i18n-translation-webpack-plugin",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gctools-outilsgc/gctools-components.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel index.js -d dist --copy-files",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-update": "npm run test -- -u",
    "test": "jest --rootDir . --config ./node_modules/@gctools-components/jest-config/index.js"
  },
  "keywords": [
    "spa",
    "localization",
    "i18n",
    "internationalization",
    "react",
    "gettext",
    "pot",
    "jed",
    "language"
  ],
  "author": {
    "name": "Luc Belliveau",
    "email": "luc.belliveau@nrc-cnrc.gc.ca"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@gctools-components/eslint-config": "^1.1.0",
    "@gctools-components/i18n-translation-webpack-plugin": "^1.1.0",
    "@gctools-components/jest-config": "^1.1.0",
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-info": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addon-storyshots": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "babel-cli": "^6.26.0",
    "babel-plugin-react-docgen": "~1.8.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@gctools-components/react-i18n-translation-webpack": "^1.1.0",
    "country-language": "^0.1.7",
    "material-ui": "^0.19.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie 11"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "react-docgen"
    ]
  },
  "eslintConfig": {
    "extends": "@gctools-components",
    "root": true
  }
}
