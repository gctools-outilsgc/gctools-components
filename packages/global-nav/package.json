    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addon-storyshots": "^3.4.0",
    "@storybook/addon-viewport": "^3.4.12",
    "@storybook/react": "^3.4.0",
