{
  "name": "@gctools-components/gcconnex-ref-impl",
  "description": "Reference Implementation of GCconnex integration",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gctools-outilsgc/gctools-components.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build-gcpedia": "webpack --config ./webpack.config.gcpedia.js",
    "build": "webpack",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint .",
    "test": "jest --rootDir . --config ./node_modules/@gctools-components/jest-config/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "gctools",
    "gctoolscomponent"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-info": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/addon-storyshots": "^3.2.13",
    "@storybook/react": "^3.3.0-alpha.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-react-docgen": "~1.8.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "@gctools-components/eslint-config": "^1.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "@gctools-components/jest-config": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "jsrsasign": "^8.0.4",
    "graphql-tag": "^2.6.1",
    "react": "^16.0.0",
    "react-apollo": "^2.0.4",
    "apollo-client": "^2.0.4",
    "apollo-link-http": "^1.3.2",
    "apollo-cache-inmemory": "^1.1.4",
    "babel-polyfill": "^6.26.0",
    "react-dom": "^16.0.0",
    "material-ui": "^0.19.4",
    "prop-types": "^15.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-runtime",
      "transform-object-rest-spread",
      [
        "react-docgen",
        {
          "includeMethods": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@gctools-components",
    "root": true
  }
}